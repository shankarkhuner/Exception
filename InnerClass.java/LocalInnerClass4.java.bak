
class Outer
{
	void m1()
		{
			class Inner
			{
				int a = 100;
			}
			Inner i = new Inner();
			System.out.println(i.a);
		}
	void m2()
		{
			class Inner
			{
				float b = 200.25f;
			}
			Inner i =  new Inner();
			System.out.println(i.b);
		}
}
class Demo 
{
	public static void main(String[] args) 
	{
		System.out.println("Start");
		Outer o = new Outer();
		o.m1();
		o.m2();
		System.out.println("Stop");
	}
}
